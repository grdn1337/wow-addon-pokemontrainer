<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

	<Button name="PokemonTrainerPetInfoTemplate" virtual="true">	
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="Icon" setAllPoints="true"/>
			</Layer>
			<Layer level="ARTWORK">
				<Texture name="$parentBorderAlive" parentKey="BorderAlive" inherits="BackupPet-Frame">
					<Anchors>
						<Anchor point="TOPLEFT" x="-5" y="5"/>
						<Anchor point="BOTTOMRIGHT" x="5" y="-5"/>
					</Anchors>
				</Texture>
				<Texture parentKey="BorderDead" inherits="BackupPet-DeadFrame">
					<Anchors>
						<Anchor point="TOPLEFT" x="-5" y="5"/>
						<Anchor point="BOTTOMRIGHT" x="5" y="-5"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Button>
	
	<Button name="PokemonTrainerPetInfoTemplate-Mini" inherits="PokemonTrainerPetInfoTemplate" virtual="true">
		<Size x="28" y="28"/>
	</Button>
	
	<Frame name="PokemonTrainerPetTemplate" virtual="true">
		<Size x="405" y="168"/>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" atlas="PetJournal-PetCard-BG">
					<Size x="405" y="166"/>	
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="0"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Button name="$parentPetInfo" parentKey="PetInfo" inherits="PokemonTrainerPetInfoTemplate">
				<Size x="38" y="38"/>
				<Anchors>
					<Anchor point="TOPLEFT" x="2" y="-2"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY" textureSubLevel="1">
						<Texture name="$parentLevelBubble" parentKey="LevelBG" atlas="PetJournal-LevelBubble" useAtlasSize="true">
							<Anchors>
								<Anchor point="BOTTOMRIGHT" relativeKey="$parent.Icon" x="5" y="-5"/>
							</Anchors>
						</Texture>
						<FontString name="$parentLevel" inherits="GameFontNormalSmall" parentKey="Level" justifyH="CENTER" text="LVL">
							<Size x="0" y="0"/>
							<Anchors>
								<Anchor point="CENTER" relativeTo="$parentLevelBubble" x="1" y="0"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
				<Frames>
					<Frame parentKey="Type">
						<Size x="28" y="28"/>
						<Anchors>
							<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parentBorderAlive" x="8" y="-2"/>
						</Anchors>
						<Layers>
							<Layer level="BACKGROUND">
								<Texture parentKey="BG" inherits="MainPet-PetFamilyFrame" setAllPoints="true"/>
							</Layer>
							<Layer level="ARTWORK">
								<Texture parentKey="Icon">
									<Size x="28" y="28"/>
									<Anchors>
										<Anchor point="CENTER"/>
									</Anchors>
									<TexCoords left="0.49218750" right="0.79687500" top="0.50390625" bottom="0.65625000"/>
								</Texture>
							</Layer>
							<Layer level="OVERLAY">
								<Texture parentKey="Active" inherits="MainPet-PetFamilyActivate">
									<Size x="36" y="36"/>
									<Anchors>
										<Anchor point="CENTER"/>
									</Anchors>
								</Texture>
							</Layer>
						</Layers>
						<Scripts>
							<OnEnter>
								local pet = self:GetParent():GetParent()._class;
								PetBattleAbilityTooltip_SetAuraID(pet:GetSide(), pet:GetSlot(), pet:GetTypePassive());
								PetBattleAbilityTooltip_Show("TOPLEFT", self, "BOTTOMRIGHT", 0, 0);
							</OnEnter>
							<OnLeave>
								PetBattlePrimaryAbilityTooltip:Hide();
							</OnLeave>
						</Scripts>
					</Frame>
				</Frames>
			</Button>
			<Frame name="$parentSpeedFrame" parentKey="SpeedFrame">
				<Size x="50" y="18"/>
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="2" y="5"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture file="Interface\PetBattles\PetBattle-StatIcons">
							<Anchors>
								<Anchor point="TOPLEFT" x="1" y="-1"/>
							</Anchors>
							<Size x="16" y="16"/>
							<TexCoords left="0.0" right="0.5" top="0.5" bottom="1.0"/>
						</Texture>
						<FontString parentKey="Text" inherits="GameFontHighlight" justifyH="LEFT" text="PET_SPEED">
							<Size x="0" y="0"/>
							<Anchors>
								<Anchor point="LEFT" x="19" y="0"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
			<Frame name="$parentPowerFrame" parentKey="PowerFrame">
				<Size x="50" y="18"/>
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" relativeTo="$parentSpeedFrame" x="0" y="0"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture file="Interface\PetBattles\PetBattle-StatIcons">
							<Anchors>
								<Anchor point="TOPLEFT" x="1" y="-1"/>
							</Anchors>
							<Size x="16" y="16"/>
							<TexCoords left="0.0" right="0.5" top="0.0" bottom="0.5"/>
						</Texture>
						<FontString parentKey="Text" inherits="GameFontHighlight" justifyH="LEFT" text="PET_POWER">
							<Size x="0" y="0"/>
							<Anchors>
								<Anchor point="LEFT" x="19" y="0"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>
			<Frame name="$parentHealthFrame" parentKey="HealthFrame">
				<Size x="50" y="18"/>
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" relativeTo="$parentPowerFrame" x="0" y="0"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture name="$parentHealthTex" file="Interface\PetBattles\PetBattle-StatIcons">
							<Anchors>
								<Anchor point="TOPLEFT" x="1" y="-1"/>
							</Anchors>
							<Size x="16" y="16"/>
							<TexCoords left="0.5" right="1.0" top="0.5" bottom="1.0"/>
						</Texture>
						<FontString name="$parentHealthValue" inherits="GameFontHighlight" justifyH="LEFT" parentKey="Text" text="PET_HEALTH">
							<Size x="0" y="0"/>
							<Anchors>
								<Anchor point="LEFT" relativeTo="$parentHealthTex" relativePoint="RIGHT" x="2" y="0"/>
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
			</Frame>			
			<PlayerModel name="$parentModelFrame" parentKey="Model" inherits="ModelTemplate">
				<Size x="173" y="135"/>
				<Anchors>
					<Anchor point="TOPLEFT" x="40" y="-21"/>
				</Anchors>
				<Scripts>
					<OnShow>
						self:SetDisplayInfo(31956);
					</OnShow>
				</Scripts>
			</PlayerModel>
		</Frames>
	</Frame>
	
	<Frame name="PokemonTrainerPetTemplate-Player" inherits="PokemonTrainerPetTemplate" virtual="true">
		<Frames>
			<Frame name="$parentSpellsFrame" parentKey="Spells">
				<Anchors>
					<Anchor point="TOPRIGHT" x="-6" y="-6"/>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMRIGHT" relativeTo="$parentModelFrame" x="0" y="-4" />
				</Anchors>
				<Layers>
					<Layer level="BACKGROUND">
						<Texture parentKey="debugTexture" setAllPoints="true">
							<Color r="0.8" g="0" b="0" a="0.5"/>
						</Texture>
					</Layer>
				</Layers>
				<Frames>
					<Button name="$parentEnemy3" inherits="PokemonTrainerPetInfoTemplate-Mini" parentKey="Enemy3">
						<Anchors>
							<Anchor point="TOPRIGHT" x="-2" y="0" />
						</Anchors>
					</Button>
					<Button name="$parentEnemy2" inherits="PokemonTrainerPetInfoTemplate-Mini" parentKey="Enemy2">
						<Anchors>
							<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$parentEnemy3" x="-10" y="0" />
						</Anchors>
					</Button>
					<Button name="$parentEnemy1" inherits="PokemonTrainerPetInfoTemplate-Mini" parentKey="Enemy1">
						<Anchors>
							<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$parentEnemy2" x="-10" y="0" />
						</Anchors>
					</Button>
					
					<Button name="$parentSpell1" inherits="PokemonTrainerAbilityButtonTemplate" id="1" parentKey="Spell1">
						<Anchors>
							<Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentEnemy1" x="-38" y="-10"/>
						</Anchors>
					</Button>
					<Button name="$parentSpell2" inherits="PokemonTrainerAbilityButtonTemplate" id="2" parentKey="Spell2">
						<Anchors>
							<Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentSpell1" x="0" y="-10"/>
						</Anchors>
					</Button>
					<Button name="$parentSpell3" inherits="PokemonTrainerAbilityButtonTemplate" id="3" parentKey="Spell3">
						<Anchors>
							<Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentSpell2" x="0" y="-10"/>
						</Anchors>
					</Button>
				</Frames>
			</Frame>
		</Frames>
	</Frame>
	
	<Frame name="PokemonTrainerPetTemplate-Enemy" inherits="PokemonTrainerPetTemplate" virtual="true">
		<Frames>
			<Frame name="$parentSpellsFrame" parentKey="Spells">
				<Anchors>
					<Anchor point="TOPRIGHT" x="-6" y="-6"/>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMRIGHT" relativeTo="$parentModelFrame" x="0" y="-4" />
				</Anchors>
				<Layers>
					<Layer level="BACKGROUND">
						<Texture parentKey="debugTexture" setAllPoints="true">
							<Color r="0.8" g="0" b="0" a="0.5"/>
						</Texture>
					</Layer>
				</Layers>
				<Frames>
					<Button name="$parentEnemy3" inherits="PokemonTrainerPetInfoTemplate-Mini" parentKey="Enemy3">
						<Anchors>
							<Anchor point="TOPRIGHT" x="-2" y="0" />
						</Anchors>
					</Button>
					<Button name="$parentEnemy2" inherits="PokemonTrainerPetInfoTemplate-Mini" parentKey="Enemy2">
						<Anchors>
							<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$parentEnemy3" x="-10" y="0" />
						</Anchors>
					</Button>
					<Button name="$parentEnemy1" inherits="PokemonTrainerPetInfoTemplate-Mini" parentKey="Enemy1">
						<Anchors>
							<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$parentEnemy2" x="-10" y="0" />
						</Anchors>
					</Button>
					
					<Button name="$parentSpell1" inherits="PokemonTrainerAbilityButtonTemplate" id="1" parentKey="Spell1">
						<Anchors>
							<Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentEnemy1" x="-38" y="-10"/>
						</Anchors>
					</Button>
					<Button name="$parentSpell2" inherits="PokemonTrainerAbilityButtonTemplate" id="2" parentKey="Spell2">
						<Anchors>
							<Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentSpell1" x="0" y="-10"/>
						</Anchors>
					</Button>
					<Button name="$parentSpell3" inherits="PokemonTrainerAbilityButtonTemplate" id="3" parentKey="Spell3">
						<Anchors>
							<Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentSpell2" x="0" y="-10"/>
						</Anchors>
					</Button>
					
					<Button name="$parentSpell4" inherits="PokemonTrainerAbilityButtonTemplate" id="4" parentKey="Spell4">
						<Anchors>
							<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$parentSpell1" x="-5" y="0"/>
						</Anchors>
					</Button>
					<Button name="$parentSpell5" inherits="PokemonTrainerAbilityButtonTemplate" id="5" parentKey="Spell5">
						<Anchors>
							<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$parentSpell2" x="-5" y="0"/>
						</Anchors>
					</Button>
					<Button name="$parentSpell6" inherits="PokemonTrainerAbilityButtonTemplate" id="6" parentKey="Spell6">
						<Anchors>
							<Anchor point="RIGHT" relativePoint="LEFT" relativeTo="$parentSpell3" x="-5" y="0"/>
						</Anchors>
					</Button>
					
				</Frames>
			</Frame>
		</Frames>
	</Frame>
	
</Ui>